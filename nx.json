{
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        "tsconfig.base.json": "*",
        "tslint.json": "*",
        ".eslintrc.json": "*",
        "nx.json": "*"
    },
    "affected": {
        "defaultBase": "master"
    },
    "npmScope": "coinage-app",
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "projects": {
        "api": {
            "tags": []
        },
        "coinage": {
            "tags": [],
            "implicitDependencies": []
        },
        "coinage-e2e": {
            "tags": [],
            "implicitDependencies": ["coinage"]
        },
        "interfaces": {
            "tags": []
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    }
}

    "scripts": {
        "ng": "nx",
        "nx": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "dev": "run-p dev:api dev:coinage",
        "dev:api": "nx serve api",
        "dev:coinage": "nx serve coinage",
        "build": "run-s build:api build:coinage build:docker",
        "build:api": "ng build api",
        "build:coinage": "ng build coinage --prod",
        "build:docker": "run-s docker:build-api docker:build-coinage",
        "docker:build-api": "docker image build -t coinage-api -f api.Dockerfile .",
        "docker:build-coinage": "docker image build -t coinage-webapp -f webapp.Dockerfile .",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "test": "ng test",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",

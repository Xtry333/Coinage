{
    "name": "coinage-app",
    "version": "0.1.0",
    "license": "MIT",
    "packageManager": "yarn@3.3.1",
    "private": true,
    "author": "Michał Woźniak <xtry333@gmail.com>",
    "repository": "git@github.com:Xtry333/Coinage.git",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "nx": "nx",
        "dev": "run-p dev:api dev:webapp",
        "dev:api": "nx serve coinage-api",
        "dev:webapp": "nx serve coinage-webapp",
        "build": "run-s build:api build:webapp build:docker",
        "build:api": "ng build coinage-api --prod",
        "build:webapp": "ng build coinage-webapp --prod",
        "build:docker": "run-s docker:build-api docker:build-webapp",
        "docker:build-api": "docker image build -t coinage-api -f api.Dockerfile .",
        "docker:build-webapp": "docker image build -t coinage-webapp -f webapp.Dockerfile .",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "test": "run-s test:api test:webapp",
        "test:api": "nx test coinage-api",
        "test:webapp": "nx test coinage-webapp",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "typeorm": "node -r dotenv/config -r ts-node node_modules/typeorm/cli-ts-node-commonjs -d packages/coinage-api/src/app/typeorm.config.ts",
        "migration:create": "typeorm migration:create",
        "update": "run-s update:latest format",
        "update:latest": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
    "dependencies": {
        "@angular/animations": "~15.0.0",
        "@angular/common": "~15.0.0",
        "@angular/compiler": "~15.0.0",
        "@angular/core": "~15.0.0",
        "@angular/forms": "~15.0.0",
        "@angular/platform-browser": "~15.0.0",
        "@angular/platform-browser-dynamic": "~15.0.0",
        "@angular/router": "~15.0.0",
        "@fortawesome/angular-fontawesome": "0.12.1",
        "@fortawesome/fontawesome-svg-core": "6.2.1",
        "@fortawesome/free-solid-svg-icons": "6.2.1",

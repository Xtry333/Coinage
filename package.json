{
    "name": "coinage-app",
    "version": "0.1.0",
    "license": "MIT",
    "packageManager": "yarn@3.2.0",
    "repository": "git@github.com:Xtry333/Coinage.git",
    "author": "Michał Woźniak <xtry333@gmail.com>",
    "private": true,
    "scripts": {
        "ng": "nx",
        "nx": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2022 browser module main",
        "dev": "run-p dev:api dev:coinage",
        "dev:api": "nx serve api",
        "dev:coinage": "nx serve coinage",
        "build": "run-s build:api build:coinage build:docker",
        "build:api": "ng build api",
        "build:coinage": "ng build coinage --prod",
        "build:docker": "run-s docker:build-api docker:build-coinage",
        "docker:build-api": "docker image build -t coinage-api -f api.Dockerfile .",
        "docker:build-coinage": "docker image build -t coinage-webapp -f webapp.Dockerfile .",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "test": "ng test",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "typeorm": "node -r dotenv/config -r ts-node node_modules/typeorm/cli-ts-node-commonjs -d apps/api/src/app/typeorm.config.ts",
        "migration:create": "yarn typeorm migration:create -n",
        "update": "run-s update:latest format",
        "update:latest": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
